{
  "name": "kilimo-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "type-check": "turbo type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "supabase:start": "cd supabase && supabase start",
    "supabase:stop": "cd supabase && supabase stop",
    "supabase:status": "cd supabase && supabase status",
    "supabase:logs": "cd supabase && supabase functions serve",
    },
  },
  "devDependencies": {
    "turbo": "^1.12.4",
    "typescript": "^5.1.6",
    "prettier": "^3.1.0"
  },
  "packageManager": "pnpm@8.9.2"
}
